{
  "description": "Replaces your newtab with the newest xkcd",
  "manifest_version": 2,
  "name": "XKCD Newtab",
  "version": "1.0",
  "homepage_url": "https://github.com/simplyluke/xkcd-newtab",

  "chrome_url_overrides" : {
    "newtab" : "xkcd-newtab.html"
  },
  "permissions": [
    "*://xkcd.com/*",
    "webRequest"
  ],
  "applications": {
    "gecko": {
      "id": "xkcd@xkcd.com",
      "strict_min_version": "0"
    }
  }
}
